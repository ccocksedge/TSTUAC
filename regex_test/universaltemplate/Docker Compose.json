    "script": "#!/opt/universal/python3.6/bin/python3\n# --\n#         Origins: Stonebranch\n#          Author: Colin Cocksedge colin.cocksedge@stonebranch.com\n#            Date: 07-Oct-2019\n#\n#    Copyright (c) Stonebranch, 2018.  All rights reserved.\n#\n#         Purpose: Universal Task for Docker Compose Projects / Services\n#\nversion = \"1.0\"\n#           Version History:    1.0     CCO     07-Oct-2019      Initial Version\n# --\n    \n# -- Main Logic Function\ndef main():\n    ScriptSetup()  # -- Import Required Modules, Setup Logging Format, Set Variables\n\n    run=DockerCompose()                  # -- Run Compose Command\n    sys.exit(run)\n\n# --\n    \n# -- Import Required Modules, Setup Logging Format, Set Variables\ndef ScriptSetup():\n    # -- Import required python modules\n    import argparse\n    import logging\n    import sys\n    import subprocess\n    import ast\n    import os\n    global logging\n    global sys\n    global subprocess\n    global ast\n    global os\n    global argparse\n    # -- Set Variables from Universal Task Form or Command Line Input\n    parser = argparse.ArgumentParser(\n        description=\"Purpose : Manage Docker Compose Projects / Services\"\n    )\n    # ## --> Capture Universal Task Form Variables Here\n    parser.add_argument(\n        \"--function\",\n        default='${ops_docker_function}',\n        help=\"Select the required docker function\",\n    choices=[\n        \"Build\",\n        \"Up\",\n        \"Down\",\n        \"Start\",\n        \"Stop\",\n    ],\n    )\n    parser.add_argument(\n        \"--project\",\n        default=\"${ops_docker_project}\",\n        help=\"Docker Compose File\",\n    )\n    parser.add_argument(\n        \"--service\",\n        default=\"${ops_docker_service}\",\n        help=\"Service Name (Optional)\",\n    )\n    parser.add_argument(\n        \"--composefile\",\n        default=r\"${_scriptpath('${ops_docker_composefile}')}\",\n        help=\"Docker Compose project name (Defaults to Current Directory)\",\n    )\n    parser.add_argument(\n        \"--timeout\",\n        default=\"${ops_docker_timeout}\",\n        help=\"Specify a shutdown timeout in seconds. (default: 10)\",\n    )\n    parser.add_argument(\n        \"--force\",\n        default=\"${ops_docker_force}\",\n        type=str2bool,\n        help=\"Always remove intermediate containers\",\n    )\n    parser.add_argument(\n        \"--compress\",\n        default=\"${ops_docker_compress}\",\n        type=str2bool,\n        help=\"Compress the build context using gzip\",\n    )\n    parser.add_argument(\n        \"--nocache\",\n        default=\"${ops_docker_cache}\",\n        type=str2bool,\n        help=\"Do not use cache when building the image\",\n    )\n    parser.add_argument(\n        \"--pull\",\n        default=\"${ops_docker_pull}\",\n        type=str2bool,\n        help=\"Always attempt to pull a newer version of the image\",\n    )\n    parser.add_argument(\n        \"--buildargs\",\n        default='{${_convertArrayFieldVariable(\"ops_docker_buildargs\",\":\",\",\",\"\\\"\",\"\\\"\")}}',\n        help=\"build arguments (--buildargs '{\"\"var1\"\":\"\"value1\"\",\"\"var2\"\":\"\"value2\"\"}')\"\n    )\n    parser.add_argument(\n        \"--detach\",\n        default=\"${ops_docker_detach}\",\n        type=str2bool,\n        help=\"Detached mode: Run containers in the background\",\n    )\n    # ## -->\n    parser.add_argument(\n        \"--loglevel\",\n        default=\"${ops_docker_loglevel}\",\n        help=\"Controls messages issued from the Script\",\n        choices=[\"None\", \"Info\", \"Debug\", \"Warning\", \"Critical\"],\n    )\n    global args\n    args = parser.parse_args()\n    # -- Setup Logging\n    numeric_level = getattr(logging, args.loglevel.upper(), None)\n    logging.basicConfig(\n        format=\"%(asctime)-15s - %(levelname)-8s - %(message)s\", level=numeric_level\n    )\n    # -- Print Paramater Values\n    logging.debug(\n        \"Executing version {0} with the following parameters : {1}\".format(\n            version, args\n        )\n    )\n    \n    \n# --\n    \n# ## --> Functions Go Here\n    \n# -- Start The Docker Project\ndef DockerCompose():\n    logging.info(\n        \"Docker Project {0} Action {1}\".format(\n            args.project,\n            args.function\n        )\n    )\n    args.function=args.function.lower()\n    # Format docker-compose command\n    options = \"\"\n    if len(args.service) != 0:\n        if str(args.service[0][:1]) == \"$\":\n            args.service=\"\"\n    else:    \n        args.service=\"\"\n    if len(args.project) != 0:\n        if str(args.project[0][:1]) != \"$\":\n            options = (\"{0} --project-name {1}\".format(options,args.project))   \n    if len(args.composefile) != 0:         \n        if str(args.composefile[0][:1]) != \"$\":\n            options = (\"{0} --file \\\"{1}\\\"\".format(options,args.composefile))       \n    if len(args.timeout) != 0:\n        if str(args.timeout[0][:1]) != \"$\":\n            args.function = (\"{0} --timeout {1}\".format(args.function,args.timeout))\n    if args.function == \"build\":\n        if args.force == True:\n            args.function = (\"{0} --force-rm\".format(args.function))\n        if args.compress == True:\n            args.function = (\"{0} --compress\".format(args.function))\n        if args.pull == True:\n            args.function = (\"{0} --pull\".format(args.function))\n        if args.nocache == True:\n            args.function = (\"{0} --no-cache\".format(args.function))\n        if len(args.buildargs) != 0:\n            if str(args.buildargs[0][:1]) != \"$\":\n                buildargs = ast.literal_eval(args.buildargs)\n                for arg, value in buildargs.items(): \n                    args.function = (\"{0} --build-arg {1}={2}\".format(args.function,arg,value))\n    if args.function == \"up\":\n        if args.detach == True:\n            args.function = (\"{0} --detach\".